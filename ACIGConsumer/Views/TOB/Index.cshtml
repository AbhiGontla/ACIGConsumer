@model Core.Api.TOB
@{
    ViewData["Title"] = "TOB";
}

@{
    var inpatient=new List<Core.Api.Inpatient>();
    var outpatient=new List<Core.Api.Outpatient>();
    var MaternityBenefit = new List<Core.Api.MaternityBenefit>();
    var DentalBenefit = new List<Core.Api.DentalBenefit>();
    var ReimbursementClaim = new List<Core.Api.ReimbursementClaim>();
    var AdditionalBenefit = new List<Core.Api.AdditionalBenefit>();
    if (Model.TOBsublist.Count>0)
    {
        List<Core.Api.TOBsublist> tOBsublists = Model.TOBsublist;
        for(int i = 0; i < tOBsublists.Count; i++)
        {
            if (tOBsublists[i].Inpatient.Count > 0)
            {
                inpatient = tOBsublists[i].Inpatient;
            }
            if (tOBsublists[i].Outpatient.Count > 0)
            {
                outpatient = tOBsublists[i].Outpatient;
            }
            if (tOBsublists[i].MaternityBenefits.Count > 0)
            {
                MaternityBenefit = tOBsublists[i].MaternityBenefits;
            }
            if (tOBsublists[i].DentalBenefits.Count > 0)
            {
                DentalBenefit = tOBsublists[i].DentalBenefits;
            }
            if (tOBsublists[i].ReimbursementClaims.Count > 0)
            {
                ReimbursementClaim = tOBsublists[i].ReimbursementClaims;
            }
            if (tOBsublists[i].AdditionalBenefits.Count > 0)
            {
                AdditionalBenefit = tOBsublists[i].AdditionalBenefits;
            }
        }
    }


}
